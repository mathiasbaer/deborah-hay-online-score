<% include ../_header %>
	<section id="main">
		<style type="text/css">
			
		</style>
		<h1><%= cell.title %></h1>
		
		Type: <%= cell.type %><br />
		
		Poster image:<br />
		<img src="http://d35vpnmjdsiejq.cloudfront.net/dh/app/cells/poster/full/<%= cell.poster %>" style="max-height:500px;max-width:500px" /><br />
		<img src="http://d35vpnmjdsiejq.cloudfront.net/dh/app/cells/poster/medium/<%= cell.poster %>" /><br />
		<img src="http://d35vpnmjdsiejq.cloudfront.net/dh/app/cells/poster/small/<%= cell.poster %>" /><br />

		Fields:<br />
		<% for ( var i = 0; i < cell.fields.length; i++ ) { %>
			<%= cell.fields[i].name %> = <%= cell.fields[i].value %><br/>
		<% } %>

		<nav><ul>
			<li><a href="/admin/cells/<%= cell.id %>/edit">Edit</a></li>
			<li><a href="/admin/cells/<%= cell.id %>/delete" confirm="Delete this cell?">Delete</a></li>
		</ul></nav>
	</section>
<% include ../_footer %>